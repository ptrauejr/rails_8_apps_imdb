.col-md-10.col-md-offset-1
  .edit-review-form
    %h1 Edit Review
    = simple_form_for([@play, @review]) do |f|
      #rating-form
        %label Rating
      = f.input :comment
      = f.button :submit, class: "btn-custom2"

:javascript
  $('#rating-form').raty({
    path: '/assets/',
    scoreName: 'review[rating]'
  });
